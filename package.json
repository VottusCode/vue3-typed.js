    "@types/typed.js": "^2.0.9",
    "cross-env": "^7.0.3",
    "postcss": "^8.2.8",
    "rollup": "^2.44.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3",
